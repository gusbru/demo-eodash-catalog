{
  "Name": "Aircraft Detection Chart",
  "Title": "Aircraft Detection Chart on-the-fly",
  "EodashIdentifier": "E13c_aircraft_detections",
  "Description": "E13c_aircraft_detections_algorithm/E13c_aircraft_detections_algorithm.md",
  "Themes": ["economy"],
  "Tags": ["Ports", "Maritime", "Traffic", "Multi-sensors"],
  "DataSource": {
    "Spaceborne": {
      "Satellite": ["Sentinel-2"],
      "Sensor": ["MSI"]
    }
  },
  "Agency": ["European Commission", "ESA"],
  "Image": "E13c_aircraft_detections_algorithm/thumbnail.png",
  "Provider": [
    {
      "Name": "Copernicus",
      "Url": "https://www.copernicus.eu/en/access-data/conventional-data-access-hubs"
    },
    {
      "Name": "Sentinel-Hub"
    }
  ],
  "References": [
    {
      "Name": "GitHub repository of vessel detection model",
      "Key": "draft_inference",
      "Url": "https://github.com/ShipDetectionExperts/draft_inference"
    }
  ],
  "Resources": [
    {
      "EndPoint": "https://services.sentinel-hub.com/ogc/wms/{SH_INSTANCE_ID}",
      "Name": "Sentinel Hub WMS",
      "LayerId": "SENTINEL-2-L2A-TRUE-COLOR",
      "DateTimeInterval": {
        "Start": "2024-01-01T00:00:00Z",
        "End": "today"
      }
    }
  ],
  "Process": {
    "Name": "E13c_aircraft_detections_algorithm",
    "JsonForm": "http://localhost:8001/processes/aircraft_detection/jsonform.json",
    "VegaDefinition": "http://localhost:8001/charts/aircraft_detection/chart.json",
    "EndPoints": [
      {
        "Identifier": "E13c_aircraft_detections",
        "Url": "http://localhost:8000/chart/json?lon_min={{bbox.0}}&lat_min={{bbox.1}}&lon_max={{bbox.2}}&lat_max={{bbox.3}}&start_date={{start_date}}&end_date={{start_date}}",
        "Type": "application/json"
      }
    ]
  }
}